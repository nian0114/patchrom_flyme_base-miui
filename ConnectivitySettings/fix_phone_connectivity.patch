     iget-object v1, p0, Lcom/meizu/connectivitysettings/MzSimInfoEditor;->mSubscriptionManager:Landroid/telephony/SubscriptionManager;

-    invoke-virtual {v1, v0}, Landroid/telephony/SubscriptionManager;->setDefaultDataSubId(I)V
+    invoke-virtual {v1, v0}, Landroid/telephony/SubscriptionManager;->setDefaultDataSubIdExtended(I)V

     return-void

--- a/smali/com/meizu/connectivitysettings/sim/SimDialogActivity.smali
+++ b/smali/com/meizu/connectivitysettings/sim/SimDialogActivity.smali
@@ -175,7 +175,7 @@
     move-result-object v0

     .local v0, "subscriptionManager":Landroid/telephony/SubscriptionManager;
-    invoke-virtual {v0, p1}, Landroid/telephony/SubscriptionManager;->setDefaultDataSubId(I)V
+    invoke-virtual {v0, p1}, Landroid/telephony/SubscriptionManager;->setDefaultDataSubIdExtended(I)V

     const v1, 0x7f0c0424

